{
  "name": "cdk-app",
  "version": "0.1.0",
  "bin": {
    "cdk-app": "bin/cdk-app.js"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "deploy": "cdk deploy --require-approval never",
    "lint": "tsc --noEmit && eslint '**/*.ts' --fix",
    "prettier-check": "prettier --check \"**/*.ts\"",
    "prettier-fix": "prettier --write \"**/*.ts\"",
    "start": "cdk synth --no-staging > template.yml && sam local start-api",
    "synth": "cdk synth > template.yml",
    "test": "jest --coverage",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "aws-cdk": "^2.0.0",
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.0.0",
    "jest-extended": "^4.0.0",
    "jest-html-reporter": "^3.0.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "esbuild": "^0.20.0",
    "lodash": "^4.0.0",
    "source-map-support": "^0.5.21"
  },
  "engines": {
    "node": "^18",
    "npm": "^9"
  }
}
